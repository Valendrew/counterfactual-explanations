        "main_camera_dual_or_triple",
        "selfie_camera_single",
        "selfie_camera_dual",
        "selfie_camera_triple"
    ],
    "subset_drop_clean": [
        "misc_price",
        "oem_model",
        "display_size",
        "has_oled_display",
        "has_memory_card_slot",
        "has_stereo_speakers",
        "has_3.5mm_jack",
        "has_wlan_5ghz",
        "has_nfc",
        "network_technology",
        "battery",
        "has_wireless_charging",
        "is_waterproof",
        "display_width",
        "display_height",
        "memory_rom_gb",
        "memory_ram_gb",
        "main_camera_resolution",
        "num_main_camera",
        "selfie_camera_resolution",
        "num_selfie_camera"
    ],
    "float_features": {
        "launch_announced": "([\\d]{4})",
        "display_size": "([\\d]{1,2}\\.[\\d]{1,2}) inches",
        "battery": "([\\d]{0,2},?[\\d]{3})(?:\\/?[\\d]{0,2},?[\\d]{3})?\\s?mah"
    },
    "binary_features": {
        "display_type": {
            "repl": [
                1,
                0
            ],
            "new_col": "has_oled_display",
            "pat": "oled"
        },
        "memory_card_slot": {
            "repl": [
                1,
                0
            ],
            "new_col": "has_memory_card_slot",
            "pat": "sd"
        },
        "sound_loudspeaker": {
            "repl": [
                1,
                0
            ],
            "new_col": "has_stereo_speakers",
            "pat": "stereo|dual|multiple|quad"
        },
        "sound_3.5mm_jack": {
            "repl": [
                1,
                0
            ],
            "new_col": "has_3.5mm_jack",
            "pat": "yes"
        },
        "comms_wlan": {
            "repl": [
                1,
                0
            ],
            "new_col": "has_wlan_5ghz",
            "pat": "(?:a\\/[b\\/gn]+)|(?:[b\\/gn]+\\/ac)"
        },
        "comms_nfc": {
            "repl": [
                1,
                0
            ],
            "new_col": "has_nfc",
            "pat": "yes"
        },
        "battery_charging": {
            "repl": [
                1,
                0
            ],
            "new_col": "has_wireless_charging",
            "pat": "wireless"
        },
        "body": {
            "repl": [
                1,
                0
            ],
            "new_col": "is_waterproof",
            "pat": "splash|water|ip[6-9]"
        }
    },
    "multi_features": {
        "network_technology": {
            "(5G)": "5G",
            "(LTE)": "4G",
            "(UMTS)": "3G",
            "(HSPA)": "3G"
        }
    },
    "multi_col_features": {
        "display_resolution": {
            "pat": "(?P<w>[\\d]{3,4}) x (?P<h>[\\d]{3,4})",
            "new_cols": {
                "w": "display_width",
                "h": "display_height"
            }
        },
        "memory_internal": {
            "pat": "^(?P<rom>[\\d]{1,3}(?:[\\.,][\\d]{1,2})?)[/\\s\\d]*GB.*?(?P<ram>[\\d]{1,2}(?:[\\.,][\\d]{1,2})?)[/\\s\\d]*GB RAM",
            "new_cols": {
                "rom": "memory_rom_gb",
                "ram": "memory_ram_gb"
            }
        }
    },
    "concat_features": {
        "oem_model": {
            "cols": [
                "oem",
                "model"
            ],
            "sep": " "
        }
    },
    "camera_features": {
        "main_camera_cols": {
            "pat": "^.*?([\\d]{1,3}(?:[\\.][\\d]{1,2})?)(?:/?[\\d]+)?\\s?mp",
            "cols": [
                "main_camera_single",
                "main_camera_dual",
                "main_camera_triple",
                "main_camera_quad",
                "main_camera_five",
                "main_camera_dual_or_triple"
            ]
        },
        "selfie_camera_cols": {
            "pat": "^.*?([\\d]{1,3}(?:[\\.][\\d]{1,2})?)(?:/?[\\d]+)?\\s?mp",
            "cols": [
                "selfie_camera_single",
                "selfie_camera_dual",
                "selfie_camera_triple"
            ]
        }
    },
    "conversion_rates": {
        "USD": 1.060668,
        "INR": 0.0114,
        "GBP": 1.0458
    },
    "fill_cols": {
        "display_type": "no",
        "memory_card_slot": "no",
        "sound_loudspeaker": "no",
        "sound_3.5mm_jack": "no",
        "comms_wlan": "no",
        "comms_nfc": "no",
        "battery_charging": "no",
        "body": "no"
    }
}